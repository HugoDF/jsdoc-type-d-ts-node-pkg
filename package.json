{
  "name": "jsdoc-type-d-ts-node-pkg",
  "description": "Example repo for a Node.js module with TypeScript typings (types.d.ts) generated from JSDoc annotations",
  "main": "src/main.js",
  "types": "./types.d.ts",
  "scripts": {
    "test": "ava",
    "build": "jsdoc -t node_modules/tsd-jsdoc/dist -r src -d .",
    "lint": "xo src tests",
    "format": "xo src tests --fix"
  },
  "devDependencies": {
    "ava": "^4.1.0",
    "jsdoc": "^3.6.10",
    "tsd-jsdoc": "^2.5.0",
    "xo": "^0.39.1"
  },
  "xo": {
    "prettier": true,
    "space": true,
    "globals": []
  },
  "license": "MIT"
}
